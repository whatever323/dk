CREATE TABLE ETUDIANT (
    CodeEt INT PRIMARY KEY,

    NomEt VARCHAR(30) NOT NULL,
    DatnEt DATE NOT NULL
);




CREATE TABLE MATIERE (
    CodeMat INT PRIMARY KEY,

    NomMat VARCHAR(15) NOT NULL,
    CoefMat INT NOT NULL
);





CREATE TABLE NOTE (
    CodeEt INT REFERENCES ETUDIANT(codeEt) ON DELETE CASCADE,
    CodeEns INT REFERENCES ENSEIGNANT(CodeEns) ON DELETE CASCADE,

    Note DECIMAL(5,2) CHECK (Note >= 0 AND Note <= 20),

);






CREATE TABLE ENSEIGNANT (
    CodeEns INT PRIMARY KEY,

    NomEns VARCHAR(30) NOT NULL,
    GradeEns VARCHAR(20),
    CodeMat INT NOT NULL,

    FOREIGN KEY (CodeMat) REFERENCES MATIERE(CodeMat)
);
